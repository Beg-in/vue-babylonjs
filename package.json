{
  "name": "vue-babylonjs",
  "version": "1.0.0-beta.6",
  "files": [
    "dist/*",
    "lib/*"
  ],
  "main": "dist/umd.js",
  "module": "dist/esm.js",
  "unpkg": "dist/umd.min.js",
  "jsdelivr": "dist/umd.min.js",
  "engines": {
    "node": ">=8.9"
  },
  "description": "A ready-to-go 3d environment for Vue.js using Babylon.js",
  "homepage": "https://github.com/Beg-in/vue-babylonjs",
  "repository": "Beg-in/vue-babylonjs",
  "bugs": {
    "url": "https://github.com/Beg-in/vue-babylonjs/issues"
  },
  "author": {
    "name": "Beg.in",
    "email": "info@beg.in",
    "url": "http://beg.in"
  },
  "contributors": [
    "Brian Jesse <brian.jesse@gmail.com> (brianjesse.com)",
    "Johnmark Beaty <johnmarkbeaty@gmail.com> (johnmarkbeaty.com)"
  ],
  "license": "MIT",
  "scripts": {
    "b": "CONTEXT=$(pwd) npm explore begin-build -- npm run",
    "start": "npm run b start:client",
    "build": "rm -rf docs && npm run b build",
    "dist:rollup": "rollup --config",
    "dist:uglify": "uglifyjs dist/umd.js -c -m -o dist/umd.min.js",
    "dist": "NODE_OPTIONS=--max_old_space_size=4096 BABEL_ENV=dist rm -rf lib dist && npm run dist:rollup && npm run dist:uglify",
    "dist:asset": "NODE_OPTIONS=--max_old_space_size=4096 npm run dist",
    "test": "BABEL_ENV=test ava test.js */test.js **/test.js",
    "test:watch": "BABEL_ENV=test npm run test -- -w",
    "test:tap": "BABEL_ENV=test npm run test -- --tap"
  },
  "peerDependencies": {
    "vue": "^2.2.0",
    "@babylonjs/core": "^4.0.0-alpha.32",
    "@babylonjs/loaders": "^4.0.0-alpha.32"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@babylonjs/core": "^4.0.0-alpha.32",
    "@babylonjs/loaders": "^4.0.0-alpha.32",
    "@vue/test-utils": "^1.0.0-beta.25",
    "ava": "^1.0.0-beta.8",
    "begin-build": "^0.16.0",
    "begin-project": "^0.4.0",
    "cannon": "^0.6.2",
    "earcut": "^2.1.3",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "find": "^0.2.9",
    "frow": "^3.0.4",
    "glslify-loader": "^2.0.0",
    "jstransformer-markdown-it": "^2.1.0",
    "jstransformer-pug": "^0.3.0",
    "markdown-it-prism": "^2.0.1",
    "oimo": "^1.0.9",
    "prismjs": "^1.15.0",
    "raw-loader": "^1.0.0",
    "rollup": "^1.4.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^4.0.1",
    "uglify-js": "^3.4.7"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  }
}
