{
  "name": "vue-babylonjs",
  "version": "1.0.0-beta.5",
  "files": [
    "dist/*",
    "lib/*"
  ],
  "main": "dist/umd.js",
  "module": "dist/esm.js",
  "unpkg": "dist/umd.min.js",
  "jsdelivr": "dist/umd.min.js",
  "engines": {
    "node": ">=8.9"
  },
  "description": "A ready-to-go 3d environment for Vue.js using Babylon.js",
  "homepage": "https://github.com/Beg-in/vue-babylonjs",
  "repository": "Beg-in/vue-babylonjs",
  "bugs": {
    "url": "https://github.com/Beg-in/vue-babylonjs/issues"
  },
  "author": {
    "name": "Beg.in",
    "email": "info@beg.in",
    "url": "http://beg.in"
  },
  "contributors": [
    "Brian Jesse <brian.jesse@gmail.com> (brianjesse.com)",
    "Johnmark Beaty <johnmarkbeaty@gmail.com> (johnmarkbeaty.com)"
  ],
  "license": "MIT",
  "scripts": {
    "b": "CONTEXT=$(pwd) npm explore begin-build -- npm run",
    "start": "npm run vendor && npm run b start:client",
    "build": "npm run vendor && rm -rf docs && npm run b build:prd",
    "dist:rollup": "rollup --config ",
    "dist:uglify": "uglifyjs dist/umd.js -c -m -o dist/umd.min.js",
    "dist": "npm run vendor && rm -rf lib dist && npm run dist:rollup && npm run dist:uglify",
    "lib:entry": "echo \"window.${GLOBAL:=$DEP} = window.${GLOBAL:=$DEP} || require('$DEP');\" >> vendor/$DEP.js",
    "lib:rollup": "webpack --mode=none ./vendor/$DEP.js --module-bind js=babel-loader -o ./vendor/$DEP.js",
    "lib": "npm run lib:entry && npm run lib:rollup",
    "vendor:earcut": "DEP=earcut npm run lib",
    "vendor:cannon": "GLOBAL=CANNON DEP=cannon npm run lib",
    "vendor:oimo": "GLOBAL=OIMO DEP=oimo npm run lib",
    "vendor": "rm -rf vendor && mkdir -p vendor && run-p vendor:*"
  },
  "peerDependencies": {
    "vue": "^2.2.0"
  },
  "dependencies": {
    "babylonjs": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "begin-build": "^0.15.0-beta.2",
    "begin-project": "0.3.1",
    "cannon": "^0.6.2",
    "earcut": "^2.1.3",
    "find": "^0.2.9",
    "frow": "^3.0.4",
    "glslify-loader": "1.0.2",
    "jstransformer-markdown-it": "^2.1.0",
    "jstransformer-pug": "^0.3.0",
    "markdown-it-prism": "^1.1.2",
    "oimo": "^1.0.9",
    "prismjs": "^1.14.0",
    "raw-loader": "0.5.1",
    "rollup": "^0.60.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "uglify-js": "3.4.0"
  },
  "// HACK: postcss-loader must have config": "",
  "postcss": {}
}
